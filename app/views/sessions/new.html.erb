<%= form_for :user do |f| %>
    <%= f.label 'Your name:' %>
    <%= f.collection_select :name, User.all, :name, :name, include_blank: 'Please select your username from this dropdown' %>
    <br /><br />
    <%= f.label :password, 'Your password:' %><br />
    <%= f.password_field :password %><br /><br />
    <%= f.submit 'Sign In' %>
<% end %>